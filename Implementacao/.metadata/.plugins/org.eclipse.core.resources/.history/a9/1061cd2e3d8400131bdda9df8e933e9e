public class Casa
{
	private String nome;
	private List<Comodo> comodos;

	public Casa(String aNome){
		this.nome = aNome;
		comodos = new ArrayList<Comodo> ();
	}

	public String getNome()
	{
		return this.nome;
	}

	public boolean putNome(String aNome)
	{
		this.nome = aNome
		if (!this.nome.equal(aNome))
			return false;
		return true;
	}

	public List<Comodo> getTodosComodos()
	{
		return this.comodos;
	}

	public boolean addNovoComodo(Comodo aComodo)
	{
		if (this.comodos.add(aComodo) == false)
			return false;
		return true;
	}

	public Comodo getComodo(String aNome)
	{
		int i;
		if (this.comodos.isEmpty() || this.aNome.isEmpty())
			return null;

		for(i = 0; i < this.comodos.size(); i++)
		{
			if( this.comodos.get(i).nome.equals(aNome) )
			{
				return this.comodos.get(i);
			}
		}
	}
}